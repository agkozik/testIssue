<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="WARN">
    <Appenders>
        <Console name="ConsoleAppender" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{[dd/MM/yyy HH:mm:ss]} :: [%level{length=5}] :: %C{2} : %M :: %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="TRACE">
            <AppenderRef ref="ConsoleAppender" />
        </Root>
    </Loggers>
</Configuration>